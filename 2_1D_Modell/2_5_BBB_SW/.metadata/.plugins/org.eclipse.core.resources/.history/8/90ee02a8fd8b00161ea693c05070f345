#include "CProxy.h"
#include "AComponentBase.h"

CProxy::CProxy() : mCommPtr(nullptr),
				   mControlPtr(nullptr)
{

}
void CProxy::registerCommComponent(AComponentBase* commComp)
{
	mCommPtr = commComp;
}
void CProxy::registerControlComponent(AComponentBase* controlComp)
{
	mControlPtr = controlComp;
}
