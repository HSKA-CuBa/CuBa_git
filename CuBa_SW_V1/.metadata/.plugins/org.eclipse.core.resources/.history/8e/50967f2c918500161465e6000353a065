#include "CControlComponent.h"

CControlComponent::CControlComponent(CProxy& proxy) : AComponentBase(proxy),
													  mFSM(proxy)
{

}
void CControlComponent::run()
{
	CMessage rxMsg;
	while(true)
	{
		if(mQueue.popFront(rxMsg, true))
		{
			mFSM.dispatch(msg);
		}
	}

}
