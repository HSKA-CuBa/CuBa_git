#include <CProxy.h>
#include "SControlComponent.h"
#include <new>

bool CProxy::timerTick(bool waitForever)
{
	CMessage msg;
	msg.setEvent(EEvent::EV_TIMER_TICK);
	return SControlComponent::getInstance().mQueue.pushFront(msg, false);
}
SProxy::SProxy()
{

}
