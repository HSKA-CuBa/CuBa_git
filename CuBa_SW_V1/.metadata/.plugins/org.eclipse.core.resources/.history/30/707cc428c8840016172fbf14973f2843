#include "CServer.h"
#include <errno.h>
#include <iostream>
#include <cstdlib>
#include <unistd.h>
#include <netinet/in.h>
#include <sys/socket.h>

CServer::CServer() : mSocketFD(-1),
					 mConnectionFD(-1)
{

}
bool CServer::waitForClient()
{
	Int32 retVal = socket(AF_INET, SOCK_STREAM, 0);
	sAssertion(retVal >= 0, "CServer::waitForClient(): Failed to create socket", true);
}
