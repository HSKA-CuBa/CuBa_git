#include <iostream>
#include "CComponent.h"

CComponent::CComponent() : mRxTask(),
						   mRxThread(&mRxTask)
{

}
void CComponent::init()
{
	std::cout << "[Component] Init" << std::endl;
	mRxThread.start();
}
void CComponent::run()
{
	while(true)
	{
		std::cout << "[Component] Run-Iteration" << std::endl;
		sleep(1);
	}
}
