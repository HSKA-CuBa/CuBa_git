#include "CServer.h"

int main()
{
	CServer server;
	server.waitForClient();
	Float32 msg = 1.0F;
	while(true)
	{
		sleep(1);
		if(false == server.transmitMessage(msg))
		{
			break;
		}
		msg += 1.0F;
	}
	server.waitForClient();
	while(true)
	{
		sleep(1);
		if(false == server.transmitMessage(msg))
		{
			break;
		}
		msg += 2.0F;
	}
	return 0;
}
