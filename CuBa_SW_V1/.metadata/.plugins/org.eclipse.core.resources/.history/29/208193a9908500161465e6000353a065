#include "AComponentBase.h"
#include "CProxy.h"

AComponentBase::AComponentBase() : mQueue(),
								 mProxyPtr(nullptr)
{

}
bool AComponentBase::setProxyPtr(CProxy& proxy)
{
	bool success = false;
	if(mProxyPtr == nullptr)
	{
		mProxyPtr = &proxy;
		success   = true;
	}
	return success;
}
