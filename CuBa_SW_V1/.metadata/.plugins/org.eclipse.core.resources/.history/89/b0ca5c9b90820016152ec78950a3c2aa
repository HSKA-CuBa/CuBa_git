/**
 * @file Global.h
 * @author Michael Meindl
 * @date 24.9.2016
 * @brief File containing the type aliases for POD.
 */
#ifndef GLOBAL_H
#define GLOBAL_H

using Int8 		= char;
using Int16		= short;
using Int32		= long;
using Int64 	= long long;
using UInt8		= unsigned char;
using UInt16	= unsigned short;
using UInt32 	= unsigned long;
using UInt64	= unsigned long long;
using Float32 	= float;
using Double64 	= double;

#endif
