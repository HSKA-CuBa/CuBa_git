#ifndef GLOBAL_H
#define GLOBAL_H
#include <stddef.h>

using UInt8 	= unsigned char;
using UInt16	= unsigned short;
using UInt32	= unsigned long;
using UInt64	= unsigned long long;
using Int8		= int8_t;
using Int16		= int16_t;
using Int32		= int32_t;
using Int64		= int64_t;

#endif
